#include <iostream>
using namespace std;

int main() {
  int n, j, z, teste = 1;
  int soma = 0;

  while (cin >> n && n) {
    cout << "Teste " << teste << endl;

    for (int i=0; i<n; i++) {
      cin >> j >> z;
      j = j + soma;
      soma = j - z;
      cout << soma << endl;
    }
    cout << endl;
    teste++;
    soma = 0;
  }


  return 0;
}
