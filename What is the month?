#include <iostream>
using namespace std;

int main() {
  
  int n;

  cin >> n;

  if (n == 1){
    cout << "Janeiro" << endl;
  } if (n == 2){
    cout << "Fevereiro" << endl;
  } if (n == 3){
    cout << "MarÃ§o" << endl;
  } if (n == 4){
    cout << "Abril" << endl;
  }if (n == 5){
    cout << "Maio" << endl;
  } if (n == 6){
    cout << "Junho" << endl;
  } if (n == 7){
    cout << "Julho" << endl;
  } if (n == 8){
    cout << "Agosto" << endl;
  } if (n == 9){
    cout << "Setembro" << endl;
  } if (n == 10){
    cout << "Outubro" << endl;
  } if (n == 11){
    cout << "Novembro" << endl;
  } if (n == 12){
    cout << "Dezembro" << endl;
  } 
 
  return 0;
}
